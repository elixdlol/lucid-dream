<Window x:Class="MainSendWindow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MainSendWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Name="beamBusCasButton" Content="Start Sending BeamBus Cas Messages" HorizontalAlignment="Left" Margin="110,100,0,0" VerticalAlignment="Top" Width="203" Click="sendBeamBusCasMessages_Click"/>
        <Button Name="allUAGButton" Content="Start Sending All UAG Messages" HorizontalAlignment="Left" Margin="110,150,0,0" VerticalAlignment="Top" Width="203" Click="sendAllMessages_Click"/>
        <Button Content="Start Sending WaterFall" HorizontalAlignment="Left" Margin="110,200,0,0" VerticalAlignment="Top" Width="203" Click="sendWaterFall_Click" />
        <Button Content="Start Sending Nav Messages" HorizontalAlignment="Left" Margin="110,250,0,0" VerticalAlignment="Top" Width="203" Click="sendNavMessages_Click" Visibility ="Hidden"/>
        <Button Content="Start Sending Bdt Cas Messages" HorizontalAlignment="Left" Margin="110,300,0,0" VerticalAlignment="Top" Width="203" Click="sendBdtCasMessages_Click" Visibility="Hidden"/>
    </Grid>
</Window>
